{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
    "OpenIdConnect": {
        "ClientId": "<your client id>",
        "Authority": "https://login.microsoftonline.com/<your tenant id>/",
        "PostLogoutRedirectUri": "https://localhost:44398/LoginHold",
